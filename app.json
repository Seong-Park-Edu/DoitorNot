{
  "expo": {
    "name": "할래말래",
    "slug": "DoItOrNot",
    "version": "1.4.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "doitornot",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.jongseong.park.DoItOrNot",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "주변 맛집 정보를 정확히 표시하기 위해 현재 위치 권한이 필요합니다."
      }
    },
    "android": {
      "versionCode": 40,
      "permissions": [
        "android.permission.INTERNET",
        "com.google.android.gms.permission.AD_ID",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "RECEIVE_BOOT_COMPLETED",
        "POST_NOTIFICATIONS",
        "VIBRATE",
        "SCHEDULE_EXACT_ALARM"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.jongseong.park.DoItOrNot",
      
      "useNextNotificationsApi": true,
      "notification": {
        "icon": "./assets/images/android-icon-monochrome.png",
        "color": "#E6F4FE",
        "iosDisplayInForeground": true
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-notifications",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-mail-composer",
      [
        "react-native-google-mobile-ads",
        {
          "androidAppId": "ca-app-pub-3217076747522132~3944850878",
          "iosAppId": "ca-app-pub-3217076747522132~3944850878"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "36895603-6a67-44fc-a9e1-fd3f9b737b0a"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-3217076747522132~3944850878",
    "ios_app_id": "ca-app-pub-3217076747522132~3944850878"
  }
}